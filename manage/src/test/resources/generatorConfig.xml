<generatorConfiguration>
    <!--id随便写-->
    <context id="frms-mgr" targetRuntime="MyBatis3">
        <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin"/>
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>
        <plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin"/>
        <!-- <plugin type="org.mybatis.generator.plugins.MapperConfigPlugin"> <property name="targetPackage" value="mybatis"
          /> <property name="targetProject" value="../rtms-orm/src/main/resources" /> </plugin> -->
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <!--去掉注释-->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!--jdbc连接驱动-->
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/mytest?characterEncoding=utf8&amp;useSSL=false&amp;serverTimezone=Asia/Shanghai&amp;rewriteBatchedStatements=true"
                        userId="root" password="root">
            <property name="nullCatalogMeansCurrent" value="true"/>
        </jdbcConnection>
        <!--默认false，把jdbc里的decimal和number类型解析为integer
          为true的时候，把这些类型转化为java.math.BigDecimal-->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <!--放pojo的位置-->
        <javaModelGenerator targetPackage="com.bysj.logistics.manage.pojo" targetProject="./src/main/java">
            <!--是否以schema作为包的后缀-->
            <property name="enableSubPackages" value="true"/>
            <!--自动去除前后空格-->
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!--xml映射文件放的位置-->
        <sqlMapGenerator targetPackage="com.bysj.logistics.manage.mapper" targetProject="./src/main/resources">
            <!--是否以schema作为包的后缀-->
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!--各种xml文件生成，固定，包位置能改-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.bysj.logistics.manage.mapper"
                             targetProject="./src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>
